{
  "manifest_version": 3,
  "name": "Notion Mail Redirect",
  "version": "1.0",
  "description": "Prompts to redirect from Gmail to Notion Mail",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
